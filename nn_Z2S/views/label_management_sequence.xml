<odoo>
    <data noupdate="1">
        <!-- Sequence for Incoming (REC-A) -->
        <record id="seq_label_management_rec_a" model="ir.sequence">
            <field name="name">Label Management REC-A Sequence</field>
            <field name="code">label_management.rec_a.sequence</field>
            <field name="prefix">REC-A-%(y)s-</field>  <!-- This will use the two-digit year format -->
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">False</field>
        </record>
        <record id="seq_label_management_internal_return_operation" model="ir.sequence">
            <field name="name">Label Management RET-INT operatuion Sequence</field>
            <field name="code">label_management.ret_int_op.sequence</field>
            <field name="prefix">RET-INT-%(y)s-</field>  <!-- This will use the two-digit year format -->
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">False</field>
        </record>


        <!-- Sequence for Incoming (REC-F) -->
        <record id="seq_label_management_rec_f" model="ir.sequence">
            <field name="name">Label Management REC-F Sequence</field>
            <field name="code">label_management.rec_f.sequence</field>
            <field name="prefix">REC-F-%(y)s-</field>  <!-- This will use the two-digit year format -->
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">False</field>
        </record>

        <!-- Sequence for Outgoing (EXP) -->
        <record id="seq_label_management_of_exp_year" model="ir.sequence">
            <field name="name">Label Management EXP Sequence</field>
            <field name="code">label_management.of_exp_year.sequence</field>
            <field name="prefix">EXP-%(y)s-</field> <!-- This will use the two-digit year format -->
            <field name="padding">4</field>  <!-- Ensures the sequence is 4 digits long -->
            <field name="number_next">1</field>
            <field name="implementation">no_gap</field>
            <field name="use_date_range">False</field>
        </record>
    </data>
</odoo>
