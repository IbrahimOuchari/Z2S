<odoo>
    <data>
        <record id="view_print_etiquette_stock_picking" model="ir.ui.view">
            <field name="name">view.print.etiquette.stock.picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="print_lot_etiquettes"
                            string="Imprimer Ã‰tiquette"
                            class="btn-primary"
                            type="object"
                            attrs="{'invisible': ['&amp;', ('picking_type_code', '!=', 'incoming'), ('operation_fourni', '!=', 'retour')]}"/>
                </xpath>
                <xpath expr="//header//button[@name='print_etiquette']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>
    </data>
</odoo>
